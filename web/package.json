    "detect-browser": "^5.3.0",
    "dexie": "^3.2.4",
    "npm-run-all": "^4.1.5",
    "qr-creator": "^1.0.0",
    "summa-wasm": "^0.129.1",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
